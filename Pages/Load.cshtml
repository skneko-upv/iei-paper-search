@page
@model IEIPaperSearch.Pages.LoadModel
@{
    ViewData["Title"] = "Motor de búsqueda bibiliográfica";
}
<h1>Carga del almacen de datos</h1>
<br>

<h4> Seleccionar parámetros de carga: </h4>
<br>

<form>
    <table style="width:40%">
        <tr>
            <td>Fuente de datos:</td>
            <td>
                <input type="checkbox" id="article_box" />
                <label> &nbsp;DBLP</label>
                <br>
                <input type="checkbox" id="book_box" />
                <label> &nbsp;IEEE Xplore</label>
                <br>
                <input type="checkbox" id="congress_box" />
                <label> &nbsp;Google Scholar</label>
                <br>
            </td>
        </tr>
    </table><br>
    <table>
                <tr>
                    <td>
                        Año: &nbsp;&nbsp; desde &nbsp;
                        <input id="year_from" />&nbsp;
                        hasta &nbsp;
                        <input id="year_to" />
                    </td>
                </tr>
            </table>
    <div> <br><br>
        <input type="submit" value="Cargar" />&nbsp;&nbsp;
        <input type="reset" value="Cancelar" />
    </div>
</form>
<br>
